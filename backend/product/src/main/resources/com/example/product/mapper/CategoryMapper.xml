<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo_new.mapper.CategoryMapper">
    <select id="findByDeletedFlagFalse">
        select * where deleted_flag is false
    </select>

    <select id="getAll">
        SELECT
        category_id,
        category_name,
        category_code
        FROM tb_category
        <where>
            deleted_flag is false
        </where>
    </select>
    </mapper>
